{
  "name": "Aliaser",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Auto-genrate aliases on the fly",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/aliaser-small.png",
    "48": "icons/aliaser-medium.png",
    "128": "icons/aliaser-large.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
